<h1>Editar Componente</h1>
<form action="/componentes/edit/<%= componente.id %>" method="POST">
    <div class="form-group">
        <label for="nome">Componentes</label>
        <input type="text" class="form-control" id="nome" name="nome" value="<%= componente.nome %>" required>
    </div>
    <button type="submit" class="btn btn-primary">Atualizar</button>
</form>